<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<managed-bean>
		<managed-bean-name>clienteBean</managed-bean-name>
		<managed-bean-class>br.com.bean.ClienteBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>mercadoriaBean</managed-bean-name>
		<managed-bean-class>br.com.bean.MercadoriaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<lifecycle>
		<phase-listener>br.com.messages.MultiPageMessagesSupport</phase-listener>
	</lifecycle>
</faces-config>
